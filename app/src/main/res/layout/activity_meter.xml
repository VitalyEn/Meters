                app:layout_constraintTop_toBottomOf="@+id/numberLayout"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textViewT1Label_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="16dp"
